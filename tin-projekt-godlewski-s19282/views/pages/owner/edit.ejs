<%- include('../../fragments/header.ejs') %>
            <main>
                <h2>Dodaj właściciela</h2>
                <form class="form" novalidate onsubmit="return validateForm()">
                    <label for="firstName">Imię:<sup>*</sup></label>
                    <input type="text" name="firstName" id="firstName" value="Jan">
                    <span id="errorFirstName" class="errors-text"></span>
                    <label for="lastName">Nazwisko:<sup>*</sup></label>
                    <input type="text" name="lastName" id="lastName" value="Kowalski">
                    <span id="errorLastName" class="errors-text"></span>
                    <label for="email">Email:</label>
                    <input type="text" name="email" id="email" value="j.kowalski@tmp.mail">
                    <span id="errorEmail" class="errors-text"></span>
                    <label for="phoneNumber">Numer telefonu:<sup>*</sup></label>
                    <input type="text" name="phoneNumber" id="phoneNumber" value="876234566">
                    <span id="errorPhoneNumber" class="errors-text"></span>
                    <div class="form-buttons">
                        <p id="errorsSummary" class="errors-text"></p>
                        <input type="submit" value="Dodaj" class="form-button-submit" />
                        <a href="/owners/" class="form-button-cancel">Anuluj</a>
                    </div>
                </form>
            </main>
<%- include('../../fragments/footer.ejs') %>